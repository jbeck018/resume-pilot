# Supabase
PUBLIC_SUPABASE_URL=your-supabase-url
PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_DB_URL=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# Cloudflare AI Gateway (optional, provides caching + analytics)
# Format: https://gateway.ai.cloudflare.com/v1/{account_id}/{gateway_name}
CLOUDFLARE_AI_GATEWAY_URL=

# LLM API Keys (direct provider access via AI SDK)
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
GOOGLE_API_KEY=your-google-api-key

# Langfuse Observability (optional, for tracing + cost analytics)
# Get keys at https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=https://cloud.langfuse.com

# Job APIs
ADZUNA_APP_ID=your-adzuna-app-id
ADZUNA_APP_KEY=your-adzuna-app-key

# Inngest
INNGEST_EVENT_KEY=your-inngest-event-key
INNGEST_SIGNING_KEY=your-inngest-signing-key

# Resend Email API (free for 100 emails/day)
# Get your API key at https://resend.com
RESEND_API_KEY=re_your_resend_api_key

# Stripe (for subscription billing)
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
STRIPE_PUBLISHABLE_KEY=pk_test_your-publishable-key

# App URL (for Stripe redirects)
PUBLIC_APP_URL=http://localhost:5173

# Claude-Flow MCP Integration (optional, enables pattern learning and swarm coordination)
# Set to 'true' to enable MCP integration
CLAUDE_FLOW_ENABLED=false
# MCP server URL (use http://localhost:3001 for local dev, or docker service name)
CLAUDE_FLOW_MCP_URL=http://localhost:3001
# Transport type: 'stdio' for local CLI, 'http' for remote server
CLAUDE_FLOW_TRANSPORT=http

# TODO: Add paid scraping services when needed
# APIFY_API_TOKEN=your-apify-token
# SCRAPINGDOG_API_KEY=your-scrapingdog-key
